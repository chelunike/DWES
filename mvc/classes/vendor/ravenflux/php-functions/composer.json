    "description": "Twig extension that implements a way to use native PHP functions and filters in Twig template",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "ravenflux",
        "twig",
        "twig-extension",
        "symfony",
        "symfony2",
        "symfony3",
        "symfony4",
        "php"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "twig/twig": "~1.27|~2.0",
        "twig/extensions": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "RavenFlux\\Twig\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
